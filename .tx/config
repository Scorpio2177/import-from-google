[main]
host = https://www.transifex.com

[import-from-google.enjson]
file_filter = client/app/locales/<lang>.json
source_file = client/app/locales/en.json
source_lang = en
trans.fr 	= client/app/locales/fr.json
type = KEYVALUEJSON

[import-from-google.server-enjson]
file_filter = server/locales/<lang>.json
source_file = server/locales/en.json
trans.fr 	= server/locales/fr.json
source_lang = en
type = KEYVALUEJSON



